<?xml version="1.0"?>
<launch>


	<arg name="debug" default="false"/>
	<arg name="headless" default="false"/>
	<arg name="paused" default="false"/>
	<arg name="use_sim_time" default="true"/>
	<arg name="gui" default="true"/>
	<arg name="throttled" default="false"/>

	<arg name="robot_env" default="bmw-assembly"/>  

	<!-- start gazebo with empty world -->
	<include file="$(find gazebo_ros)/launch/empty_world.launch">
		<arg name="world_name" value="worlds/empty.world"/>
		<arg name="debug" value="$(arg debug)" />
		<arg name="headless" value="$(arg headless)"/>
		<arg name="paused" value="$(arg paused)" />
		<arg name="use_sim_time" value="$(arg use_sim_time)" />
		<arg name="gui" value="$(arg gui)" />
		<!--arg name="throttled" value="$(arg throttled)" /-->
	</include>

	<include file="$(find cob_gazebo_worlds)/launch/world_urdf.launch">
		<arg name="robot_env" value="$(arg robot_env)"/>
	</include>

	<!-- Load joint controller configurations from YAML file to parameter server -->
  	<!--rosparam file="$(find bmw_assembly_control)/config/bmw_assembly_control.yaml" command="load" ns="bmw_assembly"/-->

	<rosparam command="load" file="$(find cob_gazebo_worlds)/urdf/$(arg robot_env)/world_controller.yaml"/>
	<node name="assembly_belt_controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" args="base_to_belt_joint_state_controller base_to_belt_joint_velocity_controller"/>


</launch>
